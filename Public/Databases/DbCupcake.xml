<?xml version="1.0" encoding="utf-8"?>
<CUPCAKE>
<DATABASE TITLE="DbCupcake" NAME="DbCupcake">
	<TABLE TITLE="TB_CR" NAME="TB_CR">
		<FIELD TYPE="NUMBER" SIZE="10" MASK="9999999999" INTERNAL_SEQUENCE="TRUE" SEQUENCE="1" TITLE="Código" NAME="ID_CR" />
		<FIELD TYPE="TEXT" SIZE="14" MASK="999.999.999-99" TITLE="Cliente" NAME="LOGIN_USER_LOGIN" />
		<FIELD TYPE="DATE" MASK="dd/MM/yyyy" TITLE="Data" NAME="DATA_CR" />
		<FIELD TYPE="TEXT" SIZE="10" TITLE="Pagamento" NAME="TPGTO_CR" />
		<FIELD TYPE="NUMBER" SIZE="10" MASK="9999999999" INTERNAL_SEQUENCE="FALSE" TITLE="Venda" NAME="ID_VENDA" />
		<FIELD TYPE="TEXT" SIZE="10" TITLE="Valor" NAME="VALOR_CR" />
		<FIELD TYPE="BOOLEAN" DEFAULT="False" TITLE="Recebido" NAME="REC_CR" />
		<INDEX TITLE="PK_TB_CR" NAME="PK_TB_CR" PRIMARY="True" UNIQUE="True" ISCLUSTER="True" FILLFACTOR="90">
			<FIELD NAME="ID_CR" />
		</INDEX>
	</TABLE>
	<TABLE TITLE="Grupo de login" NAME="TB_LOGIN_GROUP">
		<FIELD TYPE="TEXT" SIZE="60" MASK="@!" TITLE="Nome do grupo" NAME="LOGIN_GROUP_NAME" />
		<FIELD TYPE="BOOLEAN" TITLE="Grupo tipo administração" NAME="LOGIN_GROUP_IS_ADMIN" />
		<INDEX TITLE="Por nome" NAME="LOGIN_GROUP_PK" PRIMARY="True" UNIQUE="True" ISCLUSTER="True" FILLFACTOR="90">
			<FIELD NAME="LOGIN_GROUP_NAME" />
		</INDEX>
		<RELATION FOREIGNTABLE="TB_LOGIN_USER" BASETABLE="TB_LOGIN_GROUP" TYPE="CONSTRAINT" CARDINALITY="1-N" TITLE="Grupo para usuário" NAME="TB_LOGIN_GROUP_USER">
			<RELATIONOF FOREIGNFIELD="LOGIN_GROUP_NAME" BASEFIELD="LOGIN_GROUP_NAME" NAME="LOGIN_GROUP_NAME" />
		</RELATION>
	</TABLE>
	<TABLE TITLE="Usuário de login" NAME="TB_LOGIN_USER">
		<FIELD TYPE="TEXT" SIZE="60" MASK="@!" TITLE="Nome do grupo" NAME="LOGIN_GROUP_NAME" />
		<FIELD TYPE="TEXT" SIZE="14" MASK="999.999.999-99" TITLE="Login" NAME="LOGIN_USER_LOGIN" />
		<FIELD TYPE="TEXT" SIZE="40" MASK="@!" TITLE="Senha" NAME="LOGIN_USER_PASSWORD" />
		<FIELD TYPE="TEXT" SIZE="60" MASK="@!" TITLE="Nome do usuário" NAME="LOGIN_USER_NAME" />
		<FIELD TYPE="TEXT" SIZE="60" TITLE="Endereço" NAME="LOGIN_USER_ENDERECO" />
		<FIELD TYPE="TEXT" SIZE="10" TITLE="Número" NAME="LOGIN_USER_NUM" />
		<FIELD TYPE="TEXT" SIZE="60" TITLE="Complemento" NAME="LOGIN_USER_COMPL" />
		<FIELD TYPE="TEXT" SIZE="60" TITLE="Bairro" NAME="LOGIN_USER_BAIRRO" />
		<FIELD TYPE="TEXT" SIZE="60" TITLE="Cidade" NAME="LOGIN_USER_CIDADE" />
		<FIELD TYPE="TEXT" SIZE="9" MASK="99999-999" TITLE="Cep" NAME="LOGIN_USER_CEP" />
		<FIELD TYPE="TEXT" SIZE="14" MASK="(99)99999-9999" TITLE="Cel" NAME="LOGIN_USER_CEL" />
		<FIELD TYPE="MEMO" MASK="@!" TITLE="Observações" NAME="LOGIN_USER_OBS" />
		<FIELD TYPE="TEXT" SIZE="60" MASK="@a" TITLE="LOGIN_USER_EMAIL" NAME="LOGIN_USER_EMAIL" />
		<INDEX TITLE="Por login" NAME="LOGIN_USER_PK" PRIMARY="True" UNIQUE="True" ISCLUSTER="True" FILLFACTOR="90">
			<FIELD NAME="LOGIN_USER_LOGIN" />
		</INDEX>
		<RELATION FOREIGNTABLE="TB_CR" BASETABLE="TB_LOGIN_USER" TYPE="CONSTRAINT" CARDINALITY="1-N" TITLE="Usuário de login-TB_CR" NAME="FK_TB_LOGIN_USER">
			<RELATIONOF FOREIGNFIELD="LOGIN_USER_LOGIN" BASEFIELD="LOGIN_USER_LOGIN" NAME="LOGIN_USER_LOGIN" />
		</RELATION>
		<RELATION FOREIGNTABLE="TB_VENDA" BASETABLE="TB_LOGIN_USER" TYPE="CONSTRAINT" CARDINALITY="1-N" TITLE="Usuário de login-TB_VENDA" NAME="FK_TB_LOGIN_USER1">
			<RELATIONOF FOREIGNFIELD="LOGIN_USER_LOGIN" BASEFIELD="LOGIN_USER_LOGIN" NAME="LOGIN_USER_LOGIN" />
		</RELATION>
	</TABLE>
	<TABLE TITLE="TB_PRODUTO" NAME="TB_PRODUTO">
		<FIELD TYPE="NUMBER" SIZE="10" MASK="9999999999" INTERNAL_SEQUENCE="TRUE" SEQUENCE="1" TITLE="Código" NAME="ID_PRODUTO" />
		<FIELD TYPE="TEXT" SIZE="60" MASK="@!@A" TITLE="Descrição" NAME="NOME_PRODUTO" />
		<FIELD TYPE="NUMBER" SIZE="10" MASK="99.999.999,99" INTERNAL_SEQUENCE="FALSE" DEFAULT="0" TITLE="Valor" NAME="VALOR_PRODUTO" />
		<FIELD TYPE="IMAGE" TITLE="Imagem" NAME="FOTO1_PRODUTO" />
		<FIELD TYPE="IMAGE" TITLE="Imagem" NAME="FOTO2_PRODUTO" />
		<FIELD TYPE="MEMO" TITLE="Obs" NAME="OBS_PRODUTO" />
		<INDEX TITLE="PK_TB_PRODUTO" NAME="PK_TB_PRODUTO" PRIMARY="True" UNIQUE="True" ISCLUSTER="True" FILLFACTOR="90">
			<FIELD NAME="ID_PRODUTO" />
		</INDEX>
		<RELATION FOREIGNTABLE="TB_VENDA_ITEM" BASETABLE="TB_PRODUTO" TYPE="CONSTRAINT" CARDINALITY="1-N" TITLE="TB_PRODUTO-TB_VENDA_ITEM" NAME="FK_TB_PRODUTO">
			<RELATIONOF FOREIGNFIELD="ID_PRODUTO" BASEFIELD="ID_PRODUTO" NAME="ID_PRODUTO" />
		</RELATION>
	</TABLE>
	<TABLE TITLE="TB_VENDA" NAME="TB_VENDA">
		<FIELD TYPE="NUMBER" SIZE="10" MASK="9999999999" INTERNAL_SEQUENCE="TRUE" SEQUENCE="1" TITLE="Código" NAME="ID_VENDA" />
		<FIELD TYPE="TEXT" SIZE="14" MASK="999.999.999-99" TITLE="Cliente" NAME="LOGIN_USER_LOGIN" />
		<FIELD TYPE="DATETIME" MASK="dd/MM/yyyy" TITLE="Data" NAME="DATA_VENDA" />
		<FIELD TYPE="MEMO" TITLE="Obs" NAME="OBS_VENDA" />
		<FIELD TYPE="NUMBER" SIZE="10" MASK="99999999,99" INTERNAL_SEQUENCE="FALSE" DEFAULT="0" TITLE="Valor Total" NAME="TOTAL_VENDA" />
		<FIELD TYPE="TEXT" SIZE="10" TITLE="Entrega" NAME="ENTREGA_VENDA" />
		<FIELD TYPE="BOOLEAN" DEFAULT="True" TITLE="PENDENTE_VENDA" NAME="PENDENTE_VENDA" />
		<INDEX TITLE="PK_TB_VENDA" NAME="PK_TB_VENDA" PRIMARY="True" UNIQUE="True" ISCLUSTER="True" FILLFACTOR="90">
			<FIELD NAME="ID_VENDA" />
		</INDEX>
		<RELATION FOREIGNTABLE="TB_VENDA_ITEM" BASETABLE="TB_VENDA" TYPE="CONSTRAINT" CARDINALITY="1-N" TITLE="TB_VENDA-TB_VENDA_ITEM" NAME="FK_TB_VENDA">
			<RELATIONOF FOREIGNFIELD="ID_VENDA" BASEFIELD="ID_VENDA" NAME="ID_VENDA" />
		</RELATION>
	</TABLE>
	<TABLE TITLE="TB_VENDA_ITEM" NAME="TB_VENDA_ITEM">
		<FIELD TYPE="NUMBER" SIZE="10" MASK="9999999999" INTERNAL_SEQUENCE="TRUE" SEQUENCE="1" TITLE="ID_VENDA_ITEM" NAME="ID_VENDA_ITEM" />
		<FIELD TYPE="NUMBER" SIZE="10" MASK="9999999999" INTERNAL_SEQUENCE="FALSE" TITLE="ID_VENDA" NAME="ID_VENDA" />
		<FIELD TYPE="NUMBER" SIZE="10" MASK="9999999999" INTERNAL_SEQUENCE="FALSE" TITLE="ID_PRODUTO" NAME="ID_PRODUTO" />
		<FIELD TYPE="NUMBER" SIZE="10" MASK="9999999999" INTERNAL_SEQUENCE="FALSE" DEFAULT="0" TITLE="QTDE_VENDA_ITEM" NAME="QTDE_VENDA_ITEM" />
		<FIELD TYPE="NUMBER" SIZE="10" MASK="99999999,99" INTERNAL_SEQUENCE="FALSE" DEFAULT="0" TITLE="VALOR_VENDA_ITEM" NAME="VALOR_VENDA_ITEM" />
		<INDEX TITLE="PK_TB_VENDA_ITEM" NAME="PK_TB_VENDA_ITEM" PRIMARY="True" UNIQUE="True" ISCLUSTER="True" FILLFACTOR="90">
			<FIELD NAME="ID_VENDA_ITEM" />
		</INDEX>
	</TABLE>
</DATABASE>
</CUPCAKE>
